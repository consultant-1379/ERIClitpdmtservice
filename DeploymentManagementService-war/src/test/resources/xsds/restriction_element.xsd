<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
targetNamespace="http://www.ericsson.com/dmt/restriction_element"
	xmlns:dmt="http://www.ericsson.com/dmt/restriction_element">

	<simpleType name="rgb">
		<restriction base="string">
			<pattern value="((red)|(green)|(blue))" />
		</restriction>
	</simpleType>

	<element name="color" type="dmt:rgb" />

</schema>
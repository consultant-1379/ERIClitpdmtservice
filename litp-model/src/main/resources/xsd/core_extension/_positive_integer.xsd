<?xml version='1.0' encoding='utf-8'?>
<!-- litp.extensions.core_extension.CoreExtension._positive_integer -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="_positive_integer-type">
    <xs:restriction base="xs:string">
      <xs:pattern value="[1-9][0-9]*"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>

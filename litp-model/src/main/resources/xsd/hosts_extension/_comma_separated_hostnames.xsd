<?xml version='1.0' encoding='utf-8'?>
<!-- hosts_extension.hostsextension.HostsExtension._comma_separated_hostnames -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="_comma_separated_hostnames-type">
    <xs:restriction base="xs:string">
      <xs:pattern value="(\.[a-zA-Z0-9,]|[a-zA-Z0-9][a-zA-Z0-9\-,]{0,61}[a-zA-Z0-9])"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
